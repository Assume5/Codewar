
//7 kyu question: Is this a triangle?
//code
function isTriangle(a,b,c)
{
    if(a+b>c&&a+c>b&&b+c>a)
    return true;
    else
   return false;
}//bottom of isTriangle


//7kyu question:Money, Money, Money
//code
function calculateYears(principal, interest, tax, desired) {
    // your code
count=0;
    while(principal<desired){
    principal=(principal*(1+interest))-(principal*interest*tax);
    count++;
    }
    return count;
    if(principal>=desired)
    return 0;

}//bottom of calculateYears


//7ku question:Isograms
/code
function isIsogram(str){
var str=str.toUpperCase();//to uppercase(letter case dont matter)
var str=str.split("");//change it to array
var newstr="";//empty string
var i=0;
for(var j=0;j<str.length;j++){
  for(var i=j;i<str.length;i++){
    if(str[j]==str[i+1])//check
    str[i+1]="*";//if there is repeat change it to *
  }
  }//bottom of the loop

  for (i=0;i<str.length;i++){
    if(str[i]!='*' )//check
    newstr+=str[i];//store the delete letter into string
  }//bottom of loop
  if(newstr.length==str.length)
  return true;
  else if(newstr.length<str.length)
  return false;
}//bottom of isIsogram
